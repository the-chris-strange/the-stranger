{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "TheLIMSGeneratorSchema",
  "title": "Base generator schema",
  "description": "Base schema for generators included in this plugin",
  "type": "object",
  "definitions": {
    "force": {
      "type": "boolean",
      "description": "Overwrite existing files."
    },
    "skipDependencies": {
      "type": "boolean",
      "description": "Skip installing dependencies to the project.",
      "x-priority": "internal"
    },
    "skipFormat": {
      "type": "boolean",
      "description": "Skip formatting files.",
      "x-priority": "internal"
    }
  }
}
